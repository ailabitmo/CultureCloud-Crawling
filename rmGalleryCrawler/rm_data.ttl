     owl:NamedIndividual;
   rdfs:label "Портрет А.Ф. Кокоринова."@ru .

<http://rm-lod.org/object/686> a ecrm:E22_Man-Made_Object,
     owl:NamedIndividual;
   ecrm:P102_has_title <http://rm-lod.org/object/686/title/7snZzOs>;
